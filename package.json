{
  "name": "daehui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.19.0",
    "pnpm": "10.28.2"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  },
  "scripts": {
    "dev": "concurrently \"pnpm run dev:shared\" \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:shared": "pnpm -F @daehui/shared run dev",
    "dev:backend": "pnpm -F backend run start:dev",
    "dev:frontend": "pnpm -F frontend run dev",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,ts,vue,css,less,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,vue,css,less,json,md}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.2.0",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.54.0"
  }
}
